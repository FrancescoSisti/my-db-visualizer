{
  "name": "my-db-visualizer",
  "productName": "DB Visualizer Pro",
  "version": "1.0.0",
  "description": "Modern local database visualizer with intuitive UI/UX",
  "main": "src/main/index.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "concurrently \"npm run dev:renderer\" \"wait-on http://localhost:5173 && electron-forge start\"",
    "dev:renderer": "vite",
    "build": "npm run build:renderer && electron-forge package",
    "build:renderer": "vite build",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --ext .js,.ts,.vue",
    "lint:fix": "eslint src --ext .js,.ts,.vue --fix",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": ["database", "mysql", "visualizer", "electron"],
  "author": {
    "name": "Francesco Sisti",
    "email": "francescosisti61@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "mysql2": "^3.14.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "electron": "37.0.0",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/tsconfig": "^0.5.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.20.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "pinia": "^2.1.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "vue-tsc": "^1.8.0",
    "@vueuse/core": "^10.7.0",
    "@headlessui/vue": "^1.7.0",
    "@heroicons/vue": "^2.0.0",
    "wait-on": "^7.2.0"
  }
}
